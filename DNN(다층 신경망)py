  def forpass(self,x,y): (순전파를 구하는 함수)
      z1 = np.dot(x,self.w1) + self.b1
      self.a1 = self.activation(z1)
      z2 = np.dot(self.a1,self.w2) + self.b2 
      return z2
  def backprop(self,x,err): (오차를 역전파하는 함수)
      m = len(x) 
      w1_grad = np.dot(self.a1.T,err)/m
      b1_grad = np.sum(err)/m
      err_to_hidden = np.dot(self.w2.T,err)*self.a1(1-self.a1)
